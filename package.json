		"eslint-plugin-prettier": "^4.0.0",
		"pino-pretty": "^7.0.1",
		"prettier": "^2.4.1",
		"typescript": "~4.2",
		"youch": "^2.2.2",
		"youch-terminal": "^1.1.1"
	},
	"dependencies": {
